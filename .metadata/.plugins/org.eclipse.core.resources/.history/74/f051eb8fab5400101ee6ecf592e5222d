package tka_april15_Inheritance;

public class C3_Employee extends C1_Person{
	
	// id, name, age ---> extends
	String role;
	double salary;

	public C3_Employee() {
//		System.out.println("eeeeeeeeeee");
//		super(id, name, age);
	}

	public C3_Employee(int id, String name, int age, String role, double salary) {
//		super(id, name, age);
		this.role = role;
		this.salary = salary;
	}

	@Override
	public String toString() {
		System.out.println("Empty");
		return "Employee [id=" + id + ", name=" + name + ", age=" + age + ", role=" + role + ", salary=" + salary
				+ "]";
	}

//	void display() {	// readymade
		
//	};
	
	

}
