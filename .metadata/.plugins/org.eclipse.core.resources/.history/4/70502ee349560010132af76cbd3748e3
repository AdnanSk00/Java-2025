package tka_april21_Abstraction;

public abstract class C6_Manager {
	
	int id;
	double salary;
	
	public C6_Manager() {
	}
	
	public C6_Manager(int id, double salary) {
		super();
		this.id = id;
		this.salary = salary;
	}



	void direction() {		
		System.out.println("Assigning instructions...");
	}
	
	abstract void assignTask();
	
	
}
