package tka_april21_Abstraction;

public abstract class C09_Account {
	
	int accountNo;
	String name;
	float balance;
	
	public C09_Account() {
		// TODO Auto-generated constructor stub
	}
	
	
	public C09_Account(int accounNo, String name, float balance) {
		super();
		this.accountNo = accounNo;
		this.name = name;
		this.balance = balance;
	}

	abstract void deposite();
	abstract void withdraw();
	
	void display() {
		System.out.println("Account No : " + this.accountNo);
		System.out.println("Name : " + this.name);
		System.out.println("Balance : " + accountNo);
	}
	
}
