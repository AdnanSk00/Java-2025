package tka_may2;

import java.util.Scanner;

public class C92_TestArrOpretions {
	public static void main(String[] args) {
        C91_ArrayOperations arr = new C91_ArrayOperations();
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter operations : \n1. Add Elements \n2. Fist Index");
        int opr = sc.nextInt();
        
        switch (opr) {
		case 1: {
			arr.addAllElements();
			break;
		}
		case 2: {
			arr.addEleFirstIndex();;
			break;
		}
		default:
			throw new IllegalArgumentException("Unexpected value: " + key);
		}
        arr.addAllElements();

    }
}
