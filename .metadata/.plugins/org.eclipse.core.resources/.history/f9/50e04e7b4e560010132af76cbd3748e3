package tka_april21_Abstraction;

public abstract class C09_Account {
	
	int accounNo;
	String name;
	float balance;
	
	public C09_Account() {
		// TODO Auto-generated constructor stub
	}
	
	
	public C09_Account(int accounNo, String name, float balance) {
		super();
		this.accounNo = accounNo;
		this.name = name;
		this.balance = balance;
	}



	abstract void deposite();
	abstract void withdraw();
	
	abstract void display();
	
}
