package tka_april25_Static_Revision;

public class C2_A {
	
	int a = 10;
	void m1A() {
		System.out.println("A-m1()---");
	}
	
	static class NestedB{
//		class NestedC{}	// Ok
		int b = 20;
		void m1B() {
			System.out.println("B-m1()---");
		}
		
	}
	
	void m1() {		// objA --> GC -- no
		
	}
}

// We can make inner class as static but not outer class
