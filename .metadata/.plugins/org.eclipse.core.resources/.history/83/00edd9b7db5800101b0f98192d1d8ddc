package tka_may14_throws_finally;

public class FinallySyntax {
	public static void main(String[] args) {
		
//		try {
//			int a = 1 / 0;
//		}
//		finally {
//			
//		}
//		----------------------------
		try {
			
		}catch(Exception e){
			int a2 = 1 / 1;
		}
		finally {
			
		}
		
		System.out.println("thanks***");
	}
}
